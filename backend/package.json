{
  "name": "deliveryhub-api",
  "version": "1.0.0",
  "description": "DeliveryHub SaaS Platform - Multi-Delivery Provider Management API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
